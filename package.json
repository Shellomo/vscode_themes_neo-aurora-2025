{
  "name": "neo-aurora-2025-theme",
  "displayName": "Neo Aurora 2025 Theme",
  "description": "A sophisticated modern dark theme designed for the future of coding.",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Themes"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "themes": [
      {
        "label": "Neo Aurora 2025",
        "uiTheme": "vs-dark",
        "path": "./themes/neo-aurora-2025-theme.json"
      }
    ]
  },
  "publisher": "Shellomo",
  "icon": "media/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Shellomo/vscode_themes_neo-aurora-2025"
  },
  "keywords": [
    "theme",
    "dark theme",
    "modern",
    "sophisticated",
    "future",
    "2025",
    "neo",
    "aurora"
  ],
  "galleryBanner": {
    "color": "#1a1b26",
    "theme": "dark"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/node": "^16.x.x",
    "typescript": "^4.x.x"
  },
  "dependencies": {
    "@vscode/extension-telemetry": "^0.9.8"
  }
}